{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Principal": {
        "Federated": "arn:aws:iam::<account_id>:oidc-provider/oidc.eks.us-west-2.amazonaws.com/id/<oidc_provider_id>"
      },
      "Action": "sts:AssumeRoleWithWebIdentity",
      "Condition": {
        "StringEquals": {
          "oidc.eks.us-west-2.amazonaws.com/id/<id>:aud": "sts.amazonaws.com",
          "oidc.eks.us-west-2.amazonaws.com/id/<id>:sub": "system:serviceaccount:karpenter:karpenter"
        }
      }
    }
  ]
}